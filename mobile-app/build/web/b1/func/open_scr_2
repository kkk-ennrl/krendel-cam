async function oS2(){let n=new XMLHttpRequest;n.timeout=5e3,n.onerror=await(function(){dqs(".connect-screen-2").style.display="none",dqs("#blur").style.display="none"}),n.onload=await(function(){let e=JSON.parse(n.response).networks,s="";for(let n=0;n<e.length;n++)s+="<div class='network'><div class='img ",e[n].encryption>0?s+="lock":s+="unlock",s+="'></div><div class='button' onclick='oS3(\""+e[n].name+"\")'>Подключить</div>",s+="<div class='name'>"+e[n].name+"</div></div>";dqs(".connect-screen-2 .camWifi").innerHTML=s}),n.open("GET","http://192.168.4.1/scan",!0),n.send(),dqs("#blur").style.display="block",dqs("#blur").style.zIndex="999999",dqs(".connect-screen-2").style.display="block",dqs(".connect-screen-2").style.zIndex="999999"}